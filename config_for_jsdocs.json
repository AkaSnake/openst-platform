{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["tools/","services/","lib/web3","lib/contract_interact"],
    "exclude": [],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "recurseDepth": 5,
  "templates": {
    "systemName"            : "Simple Token Foundation",
    "footer"                : "Copyright 2017 OpenST Ltd.",
    "copyright"             : "",
    "includeDate"           : true,
    "theme"                 : "lumen",
    "navType"               : "vertical",
    "linenums"              : true,
    "cleverLinks"           : true,
    "monospaceLinks"        : true,
    "default": {
      "outputSourceFiles": true
    }
  },
    "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template",
    "readme": "./README.md"
  }
}